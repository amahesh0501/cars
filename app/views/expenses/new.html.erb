<div class="container">
<%= form_for [@dealership, @expense] do |f| %>
<%= render 'layouts/error_messages', object: f.object %>

<div class="form-group">
  <%= f.label :name %><br/>
  <%= f.text_field :name %>
</div>
<div class="form-group">
  <%= f.label :amount %><br/>
  <%= f.text_field :amount %>
</div>
<div class="form-group">
  <%= f.label :description%><br/>
  <%= f.text_area :description %>
</div>
<div class="form-group">
  <%= f.label :date %><br/>
  <%= f.date_field :date %>
</div>
<div class="form-group">
  <%= f.label :vendor_id %><br/>
  <%= f.collection_select :vendor_id, @vendors.all, :id, :name, prompt: true %>
</div>

<%= f.submit "Create Expense", :class => 'btn btn-success'  %>
<% end %>
</div>