<div class="container">
<%= form_for [@dealership, @expense] do |f| %>
<div class="form-group">
  <%= f.label :name %><br/>
  <%= f.text_field :name %>
</div>
<div class="form-group">
  <%= f.label :amount %><br/>
  <%= f.text_field :amount %>
</div>
<div class="form-group">
  <%= f.label :description%><br/>
  <%= f.text_area :description %>
</div>
<div class="form-group">
  <%= f.label :date %><br/>
  <%= f.date_select :date %>
</div>
<div class="form-group">
  <%= f.label :car_id %><br/>
  <%= f.collection_select :car_id, @cars.all, :id, :model, prompt: true %>
</div>
<div class="form-group">
  <%= f.label :user_id %><br/>
  <%= f.collection_select :user_id, @users.each, :id, :name, prompt: true %>
</div>

<%= f.submit "Create Expense", :class => 'btn btn-success'  %>
<% end %>
</div>