<div class="container">
<%= form_for @dealership do |f| %>
<div class="form-group">
  <%= f.label :dealership_name %><br/>
  <%= f.text_field :dealership_name %>
</div>
<div class="form-group">
  <%= f.label :dealership_address%><br/>
  <%= f.text_field :dealership_address %>
</div>
<%= f.submit "Create Dealership", :class => 'btn btn-success'  %>
<% end %>
</div>