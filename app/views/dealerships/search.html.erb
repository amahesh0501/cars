<div class="page-container"><!-- add class "sidebar-collapsed" to close sidebar by default, "chat-visible" to make chat appear always -->

  <%= render 'layouts/sidebar' %>



  <div class="main-content">
    <%= render 'layouts/dealer_admin_nav' %>

    <section class="search-results-env">

      <div class="row">
        <div class="col-md-12">


          <ul class="nav nav-tabs right-aligned">
            <li class="tab-title pull-left">
              <div class="search-string">Showing results for <strong>&ldquo;<%= @term %>&rdquo;</strong></div>
            </li>


          </ul>

          <form method="get" class="search-bar" action="/dealerships/<%= @dealership.id %>/search" enctype="application/x-www-form-urlencoded">
            <div class="input-group">
              <input type="text" class="form-control input-lg" name="search" placeholder="Search for something...">

              <div class="input-group-btn">
                <button type="submit" class="btn btn-lg btn-primary btn-icon">
                  Search
                  <i class="entypo-search"></i>
                </button>
              </div>
            </div>

          </form>


          <div class="search-results-panes">


              <ul class="search-results">
                <% if @everything.length == 0 %>
                <li class="search-result">
                  <div class="sr-inner">
                    <h4>Sorry, no results</h4>
                  </div>
                </li>
                <% else %>
                    <ul class="nav nav-tabs">
                      <li class="active"><a href="#everything" data-toggle="tab">Everything</a></li>
                      <li><a href="#vehicles" data-toggle="tab">Vehicles</a></li>
                      <li><a href="#customers" data-toggle="tab">Customers</a></li>
                      <%if @membership.is_dealership_admin %>
                      <li><a href="#employees" data-toggle="tab">Employees</a></li>
                      <% end %>
                      <li><a href="#vendors" data-toggle="tab">Partners</a></li>
                      <li><a href="#conversations" data-toggle="tab">Conversations</a></li>
                      <li><a href="#transactions" data-toggle="tab">Transactions</a></li>
                    </ul>

                    <!-- Tab panes -->
                    <div class="tab-content">
                      <div class="tab-pane active" id="everything">
                          <% if @cars %>
