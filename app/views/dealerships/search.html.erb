                              <div class="member-entry">
                               <a href="http://demo.neontheme.com/extra/timeline/" class="member-img">
                                 <img src="<%= car.image%>" class="img-rounded" />
                                 <i class="entypo-forward"></i>
                               </a>
                               <div class="member-details">
                                 <h4><%= link_to "#{car.make_model_year}", dealership_car_path(@dealership, car) %> &nbsp; <font size="2" class="result_type">Vehicle</font></h4>

                                 <div class="row info-list">

                                   <div class="col-sm-4">
                                     <p><i class="entypo-forward"></i>Vin: <%= car.vin && car.vin.length > 0 ? car.vin : "Not Entered" %></p>
                                   </div>
                                   <div class="col-sm-4">
                                     <p><i class="entypo-forward"></i>Miles: <%= car.miles && car.miles.length > 0 ? number_with_delimiter(car.miles, :delimiter => ',') : "Not Entered" %></p>
                                   </div>
                                   <div class="col-sm-4">
                                    <p><i class="entypo-forward"></i>Acqusition Date: <%= car.acquire_date ? "#{car.acquire_date.strftime("%m/%d/%Y")}" : "Not Entered" %></p>
                                  </div>
                                </div>
                              </div>
                            </div>
                          <% end %>
                        <% end %>

